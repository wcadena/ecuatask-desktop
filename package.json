{
  "name": "ecuatask-desktop",
  "version": "0.0.1",
  "author": "Wágner Alexánder Cadena Lastra <wcadena@outlook.com>",
  "description": "Sistema de actualizacion Desktop ",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "win-cert": "node .electron-vue/build.js && electron-builder create-self-signed-cert -p ecuatask"
  },
  "build": {
    "productName": "ecuatask-desktop",
    "appId": "com.ecuatask.desktop",
    "copyright": "Copyright 2021 Ecuatask.com",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "certificateFile": "private/Burp.pfx",
      "certificatePassword": "burp",
      "verifyUpdateCodeSignature": false,
      "publisherName": "ROTTEN APPLE"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@types/leaflet": "^1.5.1",
    "algoliasearch": "^4.1.0",
    "auth0-js": "^9.11.3",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.8.0",
    "core-js": "^3.4.8",
    "echarts": "^5.0.0",
    "electron-settings": "^3.2.0",
    "electron-window-state": "^5.0.3",
    "eventemitter3": "^4.0.0",
    "firebase": "^8.2.1",
    "instantsearch.css": "^7.3.1",
    "jquery": "^3.4.1",
    "jquery-scroll-lock": "^3.1.3",
    "jquery.appear": "^1.0.1",
    "js-cookie": "^2.2.1",
    "leaflet": "^1.4.0",
    "moment": "^2.22.2",
    "nprogress": "^0.2.0",
    "screenfull": "^5.1.0",
    "slick-carousel": "^1.8.1",
    "velocity-animate": "^1.5.2",
    "popper.js": "^1.16.0",
    "proglistr": "^1.4.0",
    "simplebar": "^5.0.7",
    "systeminformation": "^5.3.1",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-count-to": "^1.0.13",
    "vue-croppa": "^1.3.8",
    "vue-draggable-resizable": "^2.0.0-rc1",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-electron": "^1.0.6",
    "vue-eslint-parser": "^7.5.0",
    "vue-fullcalendar": "^1.0.9",
    "vue-fullscreen": "^2.1.5",
    "vue-i18n": "^8.14.0",
    "vue-instantsearch": "^3.4.3",
    "vue-loading-spinner": "^1.0.11",
    "vue-notification": "^1.3.16",
    "vue-perfect-scrollbar": "^0.2.0",
    "vue-quill-editor": "^3.0.6",
    "vue-radial-progress": "^0.3.2",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.2",
    "vue-server-renderer": "^2.6.12",
    "vue-slick": "^1.1.15",
    "vue-star-rating": "^1.6.1",
    "vue-tour": "^1.1.0",
